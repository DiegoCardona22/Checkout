{
  "name": "checkout-project",
  "version": "0.1.0",
  "private": true,
  "jestSonar": {
    "reportPath": "reports",
    "reportFile": "test-report.xml",
    "indent": 4
  },
  "scripts": {
    "dev": "cross-env NODE_OPTIONS='--inspect' next dev",
    "prod": "next start",
    "build": "next build",
    "build-storybook": "build-storybook -s public",
    "start": "next start",
    "lint": "eslint src",
    "test": "cross-env NODE_ENV=unitTest ./node_modules/.bin/jest --colors --env=jsdom --coverage --watchAll=false --forceExit --detectOpenHandles",
    "test:ci": "cross-env NODE_ENV=CI ./node_modules/.bin/jest",
    "storybook": "start-storybook -p 6006",
    "export": "npm run build && next export -o dist"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      },
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      },
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not chrome < 51",
      "not dead",
      "not ie 11",
      "not op_mini all",
      "not safari < 10"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "axios": "0.24.0",
    "axios-mock-adapter": "1.20.0",
    "bootstrap": "5.1.3",
    "classnames": "2.3.1",
    "cross-env": "7.0.3",
    "enzyme": "3.11.0",
    "eslint": "7.23.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-next": "12.0.1",
    "eslint-plugin-jest": "25.2.1",
    "eslint-plugin-react": "7.23.0",
    "jest": "27.3.1",
    "jest-environment-jsdom": "27.3.1",
    "jest-sonar-reporter": "2.0.0",
    "newman": "5.3.0",
    "newman-reporter-allure": "1.0.7",
    "next": "12.1.6",
    "next-redux-wrapper": "7.0.5",
    "prop-types": "15.7.2",
    "react": "18.1.0",
    "react-bootstrap": "2.4.0",
    "react-dom": "18.1.0",
    "react-redux": "8.0.2",
    "react-scripts": "4.0.3",
    "redux": "4.1.2",
    "redux-devtools-extension": "2.13.9",
    "redux-logger": "3.0.6",
    "redux-mock-store": "1.5.4",
    "redux-thunk": "2.3.0",
    "sass": "1.52.3",
    "ts-jest": "27.0.7",
    "typescript": "4.4.4",
    "web-vitals": "2.1.2",
    "webpack": "5.73.0"
  },
  "devDependencies": {
    "@mdx-js/react": "1.6.22",
    "@next/eslint-plugin-next": "12.0.1",
    "@reduxjs/toolkit": "1.8.2",
    "@storybook/addon-actions": "6.5.9",
    "@storybook/addon-essentials": "6.5.9",
    "@storybook/addon-interactions": "6.5.9",
    "@storybook/addon-links": "6.5.9",
    "@storybook/builder-webpack5": "6.5.9",
    "@storybook/manager-webpack5": "6.5.9",
    "@storybook/node-logger": "6.5.9",
    "@storybook/preset-create-react-app": "3.2.0",
    "@storybook/react": "6.5.9",
    "@storybook/testing-library": "0.0.13",
    "@types/enzyme": "3.10.10",
    "@types/jest": "27.0.2",
    "@types/lodash": "4.14.182",
    "@types/node": "12.0.0",
    "@types/react": "17.0.0",
    "@types/react-dom": "17.0.0",
    "@types/redux-logger": "3.0.9",
    "@types/redux-mock-store": "1.0.3",
    "@typescript-eslint/eslint-plugin": "5.2.0",
    "@typescript-eslint/parser": "5.2.0",
    "@wojtekmaj/enzyme-adapter-react-17": "0.6.5",
    "sb": "6.5.9"
  }
}
